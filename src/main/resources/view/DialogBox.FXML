<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<fx:root alignment="TOP_RIGHT"
         maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308"
         prefWidth="400.0"
         spacing="10.0"
         style="-fx-background-color: #e3f6f5; -fx-border-radius: 15; -fx-background-radius: 15;"
         type="javafx.scene.layout.HBox"
         xmlns="http://javafx.com/javafx/17"
         xmlns:fx="http://javafx.com/fxml/1">
  
  <children>
    <ImageView fx:id="displayPicture" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true"
               style="-fx-border-radius: 50%; -fx-background-radius: 50%;" />
    <Label fx:id="dialog" text="Label" wrapText="true" minHeight="-Infinity"
           style="-fx-font-size: 16px; -fx-font-family: 'Arial'; -fx-text-fill: #333; -fx-padding: 10px; -fx-background-color: #ffffff; -fx-border-radius: 15; -fx-background-radius: 15;" />
  </children>
  
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</fx:root>
